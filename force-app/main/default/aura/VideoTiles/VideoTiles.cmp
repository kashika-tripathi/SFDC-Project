<aura:component>
   

    <h1>this is video tile component</h1>
    <aura:handler event="c:VideoFormSubmit" action="{!c.handleChange}"/>
    
    <aura:attribute name="VideoLink" type="String"/>
    <lightning:card>
        <iframe width="420" height="315"
        
			src="{!v.VideoLink}">
    
		</iframe> 
       
        
    </lightning:card>
    <lightning:card>
        <p>{!v.VideoLink}</p>
    </lightning:card>

</aura:component>	
