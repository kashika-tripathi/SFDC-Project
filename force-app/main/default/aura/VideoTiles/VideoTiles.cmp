<aura:component>
    <h1>this is video tile component</h1>
    <aura:attribute name="videoLink" type="url"/>
    <aura:handler event="c:VideoFormSubmit" action="{!c.handleChange}"/>
   
    <lightning:card>
        <iframe width="420" height="315"  
			src="{!v.videoLink}">
		</iframe>   
        <!-- <video width="400" controls="controls">
            <source src="{!v.videoLink}" type="video/mp4"/>
        </video> -->
    </lightning:card>
    <lightning:card>
        <p>{!v.videoLink}</p>
    </lightning:card>

</aura:component>